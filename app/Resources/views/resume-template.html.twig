<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Resume - {{profile.name}}</title>

    <!-- Bootstrap core CSS -->
    <link href="{{asset('vendor/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:100,200,300,400,500,600,700,800,900"
          rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i"
          rel="stylesheet">
    <link href="{{asset('vendor/font-awesome/css/font-awesome.min.css')}}" rel="stylesheet">
    <link href="{{asset('vendor/devicons/css/devicons.min.css')}}" rel="stylesheet">
    <link href="{{asset('vendor/simple-line-icons/css/simple-line-icons.css')}}" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="{{asset('css/resume.min.css')}}" rel="stylesheet">

</head>

<body id="page-top">

<nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
    <a href="/" style="color: black;"><< Back</a>
    <a class="navbar-brand js-scroll-trigger" href="#page-top">
        <span class="d-block d-lg-none">GitHub Resume</span>
        <span class="d-none d-lg-block">
          <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="{{profile.avatar_url}}" alt="">
        </span>
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#about">About</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#languages">Languages</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#repositories">Repositories</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#contributions">Contributions</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#organizations">Organizations</a>
            </li>
            <li class="nav-item">
                <a class="nav-link js-scroll-trigger" href="#favourites">Favourites</a>
            </li>
        </ul>
    </div>
    <a href="https://github.com/BlackrockDigital/startbootstrap-resume" style="color: black; font-size: 10px;" target="_blank">Template Credit</a>
</nav>

<div class="container-fluid p-0">

    <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
        <div class="my-auto">
            <h1 class="mb-0">{{profile.name}}
                <span class="text-primary"></span>
            </h1>
            <div class="subheading mb-5">{{profile.location}} ·
                <a href="mailto:name@email.com">{{profile.email}}</a>
            </div>
            <p class="mb-5">
                {{profile.bio}}
                On GitHub since {{ profile.created_at | date('Y') }}, <a
                        href="{{ profile.html_url }}">{{ profile.name }}</a> is a developer

                based in <span class="adr locality">{{ profile.location }}</span>
                with <a href="{{ profile.html_url }}?tab=repositories">{{ profile.public_repos }} public
                    repositories</a>&nbsp;and <a href="{{ profile.html_url }}/followers">{{ profile.followers }}
                    followers
            </p>
            <ul class="list-inline list-social-icons mb-0">
                <li class="list-inline-item">
                    <a href="{{ profile.blog }}">
                <span class="fa-stack fa-lg">
                  <i class="fa fa-circle fa-stack-2x"></i>
                  <i class="fa fa-link fa-stack-1x fa-inverse"></i>
                </span>
                    </a>
                </li>
                <li class="list-inline-item">
                    <a href="{{ profile.html_url }}">
                <span class="fa-stack fa-lg">
                  <i class="fa fa-circle fa-stack-2x"></i>
                  <i class="fa fa-github fa-stack-1x fa-inverse"></i>
                </span>
                    </a>
                </li>
            </ul>
        </div>
    </section>

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="languages">
        <div class="my-auto">
            <h2 class="mb-5">Languages</h2>
            <ul class="fa-ul mb-0">
                {% for key, value in languages[0] %}
                <li>
                    <i class="fa-li fa fa-trophy text-warning"></i>
                    <a href="https://github.com/search?q=user%3A{{ profile.login }}&amp;l={{ key }}">
                        {% if key|length %} {{ key }} {% else %} Other {% endif %}
                    </a>
                    ({{ (value * 100 / languages[1]) | round }}%)
                </li>
                {% endfor %}
            </ul>
        </div>
    </section>

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="repositories">
        <div class="my-auto">
            <h2 class="mb-5">Repositories</h2>
            {% for repository in repositories %}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">{{ repository.name }}</h3>
                    <div class="subheading mb-3">{{ repository.language }}</div>
                    <p>{{ repository.description }} <a href="{{ repository.homepage }}">{{ repository.homepage }}</a></p>
                    <p>
                        This repository has {{ repository.stargazers_count }} stars and {{ repository.forks_count }}
                        forks.
                        Visit <a href="{{ repository.html_url }}">the repo</a> on GitHub.
                    </p>
                </div>
                <div class="resume-date text-md-right">
                    <span class="text-primary">{{ repository.created_at | date('Y') }} – {{ repository.updated_at | date('Y') }}</span>
                </div>
            </div>
            {% endfor %}

        </div>

    </section>

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="contributions">
        <div class="my-auto">
            <h2 class="mb-5">Contributions</h2>
            {% for contribution in contributions %}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">{{contribution.repository_name}}</h3>
                    <p>{{contribution.username}} has contributed for <a href="{{contribution.repository_url}}">{{contribution.repository_name}}</a> with <a href="{{contribution.commits_url}}">{{contribution.count}} commit(s)</a></p>
                </div>
            </div>
            {% endfor %}
        </div>
    </section>

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="organizations">
        <div class="my-auto">
            <h2 class="mb-5">Organizations</h2>
            {% for organization in organizations %}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">{{organization.login}}</h3>
                    <div class="subheading mb-3">Member</div>
                    <p>{{ organization.description }}, please visit <a
                                href="https://github.com/{{ organization.login }}">the organization page</a> on GitHub.
                    </p>
                </div>
            </div>
            {% endfor %}
        </div>
    </section>

    <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="favourites">
        <div class="my-auto">
            <h2 class="mb-5">Favourites</h2>
            {% for favourite in favourites %}
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0"><a href="{{ favourite.html_url }}">{{ favourite.full_name }}</a></h3>
                    <p>{{ favourite.description }}</p>
                </div>
            </div>
            {% endfor %}
        </div>
    </section>

</div>

<!-- Bootstrap core JavaScript -->
<script src="{{asset('vendor/jquery/jquery.min.js')}}></script>
    <script src="{{asset('vendor/bootstrap/js/bootstrap.bundle.min.js')}}></script>

<!-- Plugin JavaScript -->
<script src="{{asset('vendor/jquery-easing/jquery.easing.min.js')}}></script>

    <!-- Custom scripts for this template -->
    <script src="{{asset('js/resume.min.js')}}></script>

</body>

</html>
